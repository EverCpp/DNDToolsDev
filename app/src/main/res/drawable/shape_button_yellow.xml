<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/but_disable_bg"/>
            <stroke android:color="@color/but_disable_stroke" android:width="@dimen/but_stroke" />
            <corners android:radius="@dimen/but_corner_radius" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/but_yellow_press_bg"/>
            <stroke android:color="@color/but_yellow_press_stroke" android:width="@dimen/but_stroke" />
            <corners android:radius="@dimen/but_corner_radius" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/but_yellow_normal_bg"/>
            <stroke android:color="@color/but_yellow_normal_stroke" android:width="@dimen/but_stroke" />
            <corners android:radius="@dimen/but_corner_radius" />
        </shape>
    </item>
</selector>